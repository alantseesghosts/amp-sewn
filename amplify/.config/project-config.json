{
  "projectName": "movieswen",
  "version": "1",
  "backend": {
    "phases": {
      "build": {
        "commands": [
          "amplifyPush --simple"
        ]
      }
    }
  },
  "frontend": {
    "phases": {
      "preBuild": {
        "commands": [
          "npm ci"
        ]
      },
      "build": {
        "commands": [
          "npm run build"
        ]
      },
      "postBuild": {
        "commands": [
          "aws lambda invoke --function-name extract_sentiment 'output.txt'"
        ]
      }
    },
    "artifacts": {
      "baseDirectory": "build",
      "files": "**/*"
    },
    "cache": {
      "paths": {
        "node_modules/**/*"
      }
    }
  }
}
